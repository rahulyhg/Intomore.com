{
  "name": "final-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.16.2",
    "history": "^4.7.2",
    "moment": "^2.21.0",
    "node-sass-chokidar": "^1.3.0",
    "npm": "^6.1.0",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dotdotdot": "^1.2.3",
    "react-facebook": "^5.0.3",
    "react-helmet": "^5.2.0",
    "react-infinite-scroller": "^1.1.4",
    "react-moment": "^0.7.0",
